<div class="col-md-12">
  <div class="card card-plain">
    <div class="card-header">
      <h4 class="card-title"> <%= show_variants.crop.name %> Variants</h4>
      <p class="card-category"></p>
      <%= link_to "Add Variant", new_crop_variant_path(@crop), class: "btn btn-primary btn-sm"%>
    </div>
    <div class="card-body">
      <div class="table-responsive">
        <table class="table">
          <thead class="">
          </thead>
            <th>
              Name
            </th>
            <th>
              Description
            </th>
            <th>
              Published Journals
            </th>
            <th>
              Actions
            </th>
          <tbody>
            <tr>
              <td>
                <%= show_variants.name %>
              </td>
              <td>
                <%= show_variants.description %>
              </td>
              <td>
                55555
              </td>
              <td>
                <%= %>
                <button type="button" rel="tooltip" class="btn btn-success btn-icon btn-sm">
                  <i class="fa fa-edit"></i>
                </button>
                <button type="button" rel="tooltip" class="btn btn-danger btn-icon btn-sm">
                  <i class="fa fa-times"></i>
                </button>
              </td>
            </tr>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>